{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "tsConfigPath": "tsconfig.build.json",
    "watchAssets": true
  },
  "projects": {},
  "monorepo": false,
  "root": "src",
  "entryFile": "main",
  "exec": "node",
  "watchMode": {
    "watchAssets": true,
    "assetsExtensions": [".ts", ".js", ".json"],
    "debounceMs": 500,
    "preserveWatchOutput": true
  }
}
